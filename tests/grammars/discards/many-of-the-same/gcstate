grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority @LEX:@CHR:<0> 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@CHR:<0> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        discard {
            @DIS:@CLS:<a-z> { {priority @CLS:<a-z> 0} }
        }
        latm {
            @LEX:@CHR:<0> 1
        }
        lexeme {
            @LEX:@CHR:<0> { {priority @CHR:<0> 0} }
        }
        literal {
            @CHR:<0> { {character 48} }
            @CLS:<a-z> { {charclass {{97 122}} 0} }
        }
    }
}