grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority @LEX:@LIT:<0> 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@LIT:<0> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        discard {
            @DIS:@LCC:<a-z> { {priority @LCC:<a-z> 0} }
        }
        latm {
            @LEX:@LIT:<0> 1
        }
        lexeme {
            @LEX:@LIT:<0> { {priority @LIT:<0> 0} }
        }
        literal {
            @LCC:<a-z> { {charclass {{97 122}} 0} }
            @LIT:<0> { {string 48 0} }
        }
    }
}