grammar {
    global {
        start B
        inaccessible warn
    }
    g1 {
        {} {
            B { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        discard {
            @DIS:@CLS:<0-9> { {priority @CLS:<0-9> 0} }
        }
        latm {
            A 1
        }
        lexeme {
            A { {priority @CHR:<a> 0} }
        }
        literal {
            @CHR:<a> { {character 97} }
            @CLS:<0-9> { {charclass {{48 57}} 0} }
        }
    }
}