grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority @LEX:@LCC:<A-Z>:i 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@LCC:<A-Z>:i { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        {} {
            @LCC:<A-Z>:i { {charclass {{65 90}} 1} }
        }
        latm {
            @LEX:@LCC:<A-Z>:i 1
        }
        lexeme {
            @LEX:@LCC:<A-Z>:i { {priority @LCC:<A-Z>:i 0} }
        }
    }
}
