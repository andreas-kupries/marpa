grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority @LEX:@LCC:<0-9> 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@LCC:<0-9> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            @LEX:@LCC:<0-9> 1
        }
        lexeme {
            @LEX:@LCC:<0-9> { {priority @LCC:<0-9> 0} }
        }
        literal {
            @LCC:<0-9> { {charclass {{48 57}} 0} }
        }
    }
}