grammar {
    global {
        start B
        inaccessible warn
    }
    g1 {
        {} {
            B { {priority @LEX:@LIT:<b> 0
                    action {special first}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@LIT:<b> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            @LEX:@LIT:<b> 1
        }
        lexeme {
            @LEX:@LIT:<b> { {priority @LIT:<b> 0} }
        }
        literal {
            @LIT:<b> { {string 98 0} }
        }
    }
}