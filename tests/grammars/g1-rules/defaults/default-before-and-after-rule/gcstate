grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority @LEX:@STR:<a> 0
                    action {cmd this}
                    assoc left
                    bless {special undef}
                    mask 0} }
            B { {priority @LEX:@STR:<b> 0
                    action {cmd that}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@STR:<a> { terminal }
            @LEX:@STR:<b> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            @LEX:@STR:<a> 1
            @LEX:@STR:<b> 1
        }
        lexeme {
            @LEX:@STR:<a> { {priority @STR:<a> 0} }
            @LEX:@STR:<b> { {priority @STR:<b> 0} }
        }
        literal {
            @STR:<a> { {string 97 0} }
            @STR:<b> { {string 98 0} }
        }
    }
}