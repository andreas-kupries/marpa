grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority {
                    B
                    C
                    D
                    E} 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask {0 1 1 0}} }
        }
        terminal {
            B { terminal }
            C { terminal }
            D { terminal }
            E { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            B 1
            C 1
            D 1
            E 1
        }
        lexeme {
            B { {priority @CHR:<b> 0} }
            C { {priority @CHR:<c> 0} }
            D { {priority @CHR:<d> 0} }
            E { {priority @CHR:<e> 0} }
        }
        literal {
            @CHR:<b> { {character 98} }
            @CHR:<c> { {character 99} }
            @CHR:<d> { {character 100} }
            @CHR:<e> { {character 101} }
        }
    }
}