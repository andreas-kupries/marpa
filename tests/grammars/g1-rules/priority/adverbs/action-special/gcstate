grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority B 0
                    action {special first}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            B { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            B 1
        }
        lexeme {
            B { {priority @STR:<b> 0} }
        }
        literal {
            @STR:<b> { {string 98 0} }
        }
    }
}