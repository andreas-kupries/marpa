grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority {
                    PO
                    A
                    PC} 0
                        action {array values}
                        assoc group
                        bless {special undef}
                        mask {0 0 0}} }
        }
        terminal {
            PC { terminal }
            PO { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            PC 1
            PO 1
        }
        lexeme {
            PC { {priority {{@STR:<\51>}} 0} }
            PO { {priority {{@STR:<\50>}} 0} }
        }
        literal {
            {@STR:<\50>} { {string 40 0} }
            {@STR:<\51>} { {string 41 0} }
        }
    }
}