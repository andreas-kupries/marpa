grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority {
                    A
                    OP
                    A} 0
                        action {array values}
                        assoc right
                        bless {special undef}
                        mask {0 0 0}} }
        }
        terminal {
            OP { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            OP 1
        }
        lexeme {
            OP { {priority @CHR:<b> 0} }
        }
        literal {
            @CHR:<b> { {character 98} }
        }
    }
}