grammar {
    global {
        start E
        inaccessible warn
    }
    g1 {
        {} {
            E {
                {priority N 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask 0}
                {priority {
                        PO
                        E
                        PC} 0
                            action {array values}
                            assoc group
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E
                        POW
                        E} -1
                            action {array values}
                            assoc right
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E
                        MUL
                        E} -2
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E
                        DIV
                        E} -2
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E
                        ADD
                        E} -3
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E
                        SUB
                        E} -3
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
            }
        }
        terminal {
            ADD { terminal }
            DIV { terminal }
            MUL { terminal }
            N { terminal }
            PC { terminal }
            PO { terminal }
            POW { terminal }
            SUB { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            ADD 1
            DIV 1
            MUL 1
            N 1
            PC 1
            PO 1
            POW 1
            SUB 1
        }
        lexeme {
            ADD { {priority @CHR:<+> 0} }
            DIV { {priority @CHR:</> 0} }
            MUL { {priority @CHR:<*> 0} }
            N { {quantified @CLS:<0-9> 1} }
            PC { {priority {{@CHR:<\51>}} 0} }
            PO { {priority {{@CHR:<\50>}} 0} }
            POW { {priority @STR:<**> 0} }
            SUB { {priority @CHR:<-> 0} }
        }
        literal {
            @CHR:<*> { {character 42} }
            @CHR:<+> { {character 43} }
            @CHR:<-> { {character 45} }
            @CHR:</> { {character 47} }
            {@CHR:<\50>} { {character 40} }
            {@CHR:<\51>} { {character 41} }
            @CLS:<0-9> { {charclass {{48 57}} 0} }
            @STR:<**> { {string {42 42} 0} }
        }
    }
}