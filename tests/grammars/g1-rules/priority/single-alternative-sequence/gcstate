grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority {
                    B
                    C
                    D
                    E} 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask {0 0 0 0}} }
        }
        terminal {
            B { terminal }
            C { terminal }
            D { terminal }
            E { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            B 1
            C 1
            D 1
            E 1
        }
        lexeme {
            B { {priority @STR:<b> 0} }
            C { {priority @STR:<b> 0} }
            D { {priority @STR:<b> 0} }
            E { {priority @STR:<b> 0} }
        }
        literal {
            @STR:<b> { {string 98 0} }
        }
    }
}