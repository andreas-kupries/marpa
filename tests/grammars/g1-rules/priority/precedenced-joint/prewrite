grammar {
    global {
        start E
        inaccessible warn
    }
    g1 {
        {} {
            E { {priority E((3)) 0
                    action {special hide}
                    assoc left
                    bless {standard undef}
                    mask 0} }
            E((0)) {
                {priority N 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask 0}
                {priority {
                        PO
                        E((3))
                        PC} 0
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
            }
            E((1)) {
                {priority E((0)) 0
                        action {special hide}
                        assoc left
                        bless {standard undef}
                        mask 0}
                {priority {
                        E((0))
                        POW
                        E((1))} 0
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
            }
            E((2)) {
                {priority E((1)) 0
                        action {special hide}
                        assoc left
                        bless {standard undef}
                        mask 0}
                {priority {
                        E((2))
                        MUL
                        E((1))} 0
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E((2))
                        DIV
                        E((1))} 0
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
            }
            E((3)) {
                {priority E((2)) 0
                        action {special hide}
                        assoc left
                        bless {standard undef}
                        mask 0}
                {priority {
                        E((3))
                        ADD
                        E((2))} 0
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
                {priority {
                        E((3))
                        SUB
                        E((2))} 0
                            action {array values}
                            assoc left
                            bless {special undef}
                            mask {0 0 0}}
            }
        }
        terminal {
            ADD { terminal }
            DIV { terminal }
            MUL { terminal }
            N { terminal }
            PC { terminal }
            PO { terminal }
            POW { terminal }
            SUB { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            ADD 1
            DIV 1
            MUL 1
            N 1
            PC 1
            PO 1
            POW 1
            SUB 1
        }
        lexeme {
            ADD { {priority @CHR:<+> 0} }
            DIV { {priority @CHR:</> 0} }
            MUL { {priority @CHR:<*> 0} }
            N { {quantified @RAN:<09> 1} }
            PC { {priority {{@CHR:<\51>}} 0} }
            PO { {priority {{@CHR:<\50>}} 0} }
            POW { {priority @STR:<**> 0} }
            SUB { {priority @CHR:<-> 0} }
        }
        literal {
            @CHR:<*> { {character 42} }
            @CHR:<+> { {character 43} }
            @CHR:<-> { {character 45} }
            @CHR:</> { {character 47} }
            {@CHR:<\50>} { {character 40} }
            {@CHR:<\51>} { {character 41} }
            @RAN:<09> { {range 48 57} }
            @STR:<**> { {string 42 42} }
        }
    }
}
