grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority @LEX:@CLS/i:<A-Z> 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            @LEX:@CLS/i:<A-Z> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            @LEX:@CLS/i:<A-Z> 1
        }
        lexeme {
            @LEX:@CLS/i:<A-Z> { {priority @CLS/i:<A-Z> 0} }
        }
        literal {
            @CLS/i:<A-Z> { {charclass {{65 90}} 1} }
        }
    }
}