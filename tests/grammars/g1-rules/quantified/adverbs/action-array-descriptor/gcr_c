grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {quantified B 1
                    action {array {start length}}
                    bless {special undef}} }
        }
        terminal {
            B { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            B 1
        }
        lexeme {
            B { {priority @CHR:<b> 0} }
        }
        literal {
            @CHR:<b> { {byte 98} }
        }
    }
}