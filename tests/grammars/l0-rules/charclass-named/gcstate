grammar {
    global {
        start lang
        inaccessible warn
    }
    g1 {
        {} {
            lang { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            A 1
        }
        lexeme {
            A { {priority {{@CLS:<[:xdigit:]Q>}} 0} }
        }
        literal {
            {@CLS:<[:xdigit:]Q>} { {charclass {:xdigit 81} 0} }
        }
    }
}