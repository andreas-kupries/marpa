grammar {
    global {
        start F
        inaccessible warn
    }
    g1 {
        {} {
            F { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            A 1
        }
        lexeme {
            A { {priority {} 0
                    name reduction} }
        }
    }
}
