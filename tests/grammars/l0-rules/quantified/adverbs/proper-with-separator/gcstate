grammar {
    global {
        start D
        inaccessible warn
    }
    g1 {
        {} {
            D { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        {} {
            @LIT:<b> { {string 98 0} }
            @LIT:<c> { {string 99 0} }
            B { {priority @LIT:<b> 0} }
            C { {priority @LIT:<c> 0} }
        }
        latm {
            A 1
        }
        lexeme {
            A { {quantified B 1
                    separator {C 0}} }
        }
    }
}
