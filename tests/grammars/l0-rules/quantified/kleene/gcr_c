grammar {
    global {
        start C
        inaccessible warn
    }
    g1 {
        {} {
            C { {priority A 0
                    action {array values}
                    assoc left
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        {} {
            B { {priority @CHR:<b> 0} }
        }
        latm {
            A 1
        }
        lexeme {
            A { {quantified B 0} }
        }
        literal {
            @CHR:<b> { {byte 98} }
        }
    }
}