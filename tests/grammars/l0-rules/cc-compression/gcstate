grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A {
                {priority @LEX:@CLS:<0-1a-|> 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask 0}
                {priority @LEX:@CLS:<0-9> 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask 0}
            }
        }
        terminal {
            @LEX:@CLS:<0-1a-|> { terminal }
            @LEX:@CLS:<0-9> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            @LEX:@CLS:<0-1a-|> 1
            @LEX:@CLS:<0-9> 1
        }
        lexeme {
            @LEX:@CLS:<0-1a-|> { {priority @CLS:<0-1a-|> 0} }
            @LEX:@CLS:<0-9> { {priority @CLS:<0-9> 0} }
        }
        literal {
            @CLS:<0-1a-|> { {charclass {{48 49} {97 124}} 0} }
            @CLS:<0-9> { {charclass {{48 57}} 0} }
        }
    }
}
