grammar {
    global {
        start Language
        inaccessible warn
    }
    g1 {
        {} {
            Language { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        {} {
            @STR:<string> { {priority {
                    @BYTE:<s>
                    @BYTE:<t>
                    @BYTE:<r>
                    @BYTE:<i>
                    @BYTE:<n>
                    @BYTE:<g>} 0} }
        }
        latm {
            A 1
        }
        lexeme {
            A { {priority @STR:<string> 0} }
        }
        literal {
            @BYTE:<g> { {byte 103} }
            @BYTE:<i> { {byte 105} }
            @BYTE:<n> { {byte 110} }
            @BYTE:<r> { {byte 114} }
            @BYTE:<s> { {byte 115} }
            @BYTE:<t> { {byte 116} }
        }
    }
}