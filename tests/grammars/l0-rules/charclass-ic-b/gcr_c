grammar {
    global {
        start Language
        inaccessible warn
    }
    g1 {
        {} {
            Language { {priority A 0
                    action {array values}
                    assoc left
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        {} {
            {@CLS:<A-Za-z\u0130-\u0131\u017f\u212a>} {
                {priority @BRAN:<AZ> 0}
                {priority @BRAN:<az> 0}
                {priority {
                        {@BYTE:<\304>}
                        {@BRAN:<\260\261>}} 0}
                {priority {
                        {@BYTE:<\305>}
                        {@BYTE:<\277>}} 0}
                {priority {
                        {@BYTE:<\342>}
                        {@BYTE:<\204>}
                        {@BYTE:<\252>}} 0}
            }
        }
        latm {
            A 1
        }
        lexeme {
            A { {priority {{@CLS:<A-Za-z\u0130-\u0131\u017f\u212a>}} 0} }
        }
        literal {
            {@BRAN:<\260\261>} { {brange 176 177} }
            @BRAN:<AZ> { {brange 65 90} }
            @BRAN:<az> { {brange 97 122} }
            {@BYTE:<\204>} { {byte 132} }
            {@BYTE:<\252>} { {byte 170} }
            {@BYTE:<\277>} { {byte 191} }
            {@BYTE:<\304>} { {byte 196} }
            {@BYTE:<\305>} { {byte 197} }
            {@BYTE:<\342>} { {byte 226} }
        }
    }
}
