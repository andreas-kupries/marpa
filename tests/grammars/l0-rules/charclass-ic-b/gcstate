grammar {
    global {
        start Language
        inaccessible warn
    }
    g1 {
        {} {
            Language { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            A 1
        }
        lexeme {
            A { {priority @CLS/i:<A-Z> 0} }
        }
        literal {
            @CLS/i:<A-Z> { {charclass {{65 90}} 1} }
        }
    }
}