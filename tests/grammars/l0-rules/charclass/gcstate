grammar {
    global {
        start Language
        inaccessible warn
    }
    g1 {
        {} {
            Language { {priority A 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            A 1
        }
        lexeme {
            A { {priority @LCC:<0-9> 0} }
        }
        literal {
            @LCC:<0-9> { {charclass {{48 57}} 0} }
        }
    }
}