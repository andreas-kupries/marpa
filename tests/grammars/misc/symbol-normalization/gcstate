grammar {
    global {
        start {op comma}
        inaccessible warn
    }
    g1 {
        {} {
            {op comma} { {priority {{op comma}} 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
    }
    lexeme {
        action {array values}
    }
}