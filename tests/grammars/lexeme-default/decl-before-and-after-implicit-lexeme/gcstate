grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority {
                    foo
                    bar} 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask {0 0}} }
        }
        terminal {
            bar { terminal }
            foo { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        {} {
            @LCC:<a> { {charclass 97 0} }
            @LIT:<ab> { {string {97 98} 0} }
        }
        latm {
            bar 0
            foo 0
        }
        lexeme {
            bar { {priority @LCC:<a> 0} }
            foo { {priority @LIT:<ab> 0} }
        }
    }
}
