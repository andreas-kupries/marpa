grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority {
                    foo
                    bar} 0
                        action {array values}
                        assoc left
                        bless {special undef}
                        mask {0 0}} }
        }
        terminal {
            bar { terminal }
            foo { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            bar 0
            foo 1
        }
        lexeme {
            bar { {priority @CHR:<b> 0} }
            foo { {priority @CHR:<f> 0} }
        }
        literal {
            @CHR:<b> { {byte 98} }
            @CHR:<f> { {byte 102} }
        }
    }
}