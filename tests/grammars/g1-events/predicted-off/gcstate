    g1 {
        {} {
            A { {priority @LEX:@STR:<a> 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        events {
            A {
                predicted {
                    foo off
                }
            }
        }
        terminal {
            @LEX:@STR:<a> { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        latm {
            @LEX:@STR:<a> 1
        }
        lexeme {
            @LEX:@STR:<a> { {priority @STR:<a> 0} }
        }
