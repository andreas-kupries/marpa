grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority B 0
                    action {array values}
                    assoc left
                    mask 0} }
        }
        events {
            A {
                completed {
                    fox on
                }
            }
        }
        terminal {
            B { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        discard {
            C { {priority @CHR:<c> 0} }
        }
        events {
            C {
                discard {
                    snafu on
                }
            }
        }
        latm {
            B 1
        }
        lexeme {
            B { {priority @CHR:<a> 0} }
        }
        literal {
            @CHR:<a> { {character 97} }
            @CHR:<c> { {character 99} }
        }
    }
}