                            action {array {start length values}}
                            assoc left
                            bless {special lhs}
                            mask {1 1 0}}
                {priority {
                        @LEX:@STR:<start>
                        @LEX:@STR:<symbol>
                        @LEX:@STR:<is>
                        symbol} 0
                            action {array {start length values}}
                            assoc left
                            bless {special lhs}
                            mask {1 1 1 0}}
            }
            statement {
                {priority {{start rule}} 0
                        action {array {start length values}}
                        assoc left
                        bless {special lhs}
                        mask 0}
                {priority {{empty rule}} 0
                        action {array {start length values}}
                        assoc left
