grammar {
    global {
        start statements
        inaccessible warn
    }
    g1 {
        {} {
            action { {priority {
                    @LEX:@LIT:<action>
                    @LEX:@LIT:<=>>
                    {action name}} 0
                        action {array {start length values}}
                        assoc left
                        bless {special lhs}
                        mask {1 1 0}} }
            {action name} {
                {priority {{Perl name}} 0
                        action {array {start length values}}
                        assoc left
                        bless {special lhs}
