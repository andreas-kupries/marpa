            {statement group} { {priority {
                    {@LEX:@CHR:<\173>}
                    statements
                    {@LEX:@CHR:<\175>}} 0
                        action {array {start length values}}
                        assoc left
                        bless {special lhs}
                        mask {1 0 0}} }
            statements { {quantified statement 1
                    action {array {start length values}}
                    bless {special lhs}} }
