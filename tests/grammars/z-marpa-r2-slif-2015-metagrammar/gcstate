    l0 {
        {} {
            {array descriptor left bracket} {
                {priority {{@CHR:<\133>}} 0}
                {priority {
                        {@CHR:<\133>}
                        whitespace} 0}
            }
            {array descriptor right bracket} {
                {priority {{@CHR:<\135>}} 0}
                {priority {
                        whitespace
                        {@CHR:<\135>}} 0}
            }
            {bracketed name string} { {quantified @CLS:<sw> 1} }
            {cc element} {
                {priority {{safe cc character}} 0}
                {priority {{escaped cc character}} 0}
                {priority {{posix char class}} 0}
                {priority {{negated posix char class}} 0}
            }
            {cc elements} { {quantified {cc element} 1} }
            {character class modifier} {
                {priority @STR:<:ic> 0}
                {priority @STR:<:i> 0}
            }
            {character class modifiers} { {quantified {character class modifier} 0} }
            {double colon} { {priority @STR:<::> 0} }
            {escaped cc character} { {priority {
                    {@CHR:<\134>}
                    {horizontal character}} 0} }
            {hash comment body} { {quantified {hash comment char} 0} }
            {hash comment char} { {priority {{@^CLS:<\n-\r\u2028-\u2029>}} 0} }
            {horizontal character} { {priority {{@^CLS:<\n-\r\u2028-\u2029>}} 0} }
            integer { {quantified @CHR:<d> 1} }
            {negated posix char class} { {priority {
                    {@STR:<\133:^>}
                    {posix char class name}
                    {@STR:<:\135>}} 0} }
            {one or more word characters} { {quantified @CHR:<w> 1} }
            {Perl identifier} { {quantified @CHR:<w> 1} }
            {posix char class} { {priority {
                    {@STR:<\133:>}
                    {posix char class name}
                    {@STR:<:\135>}} 0} }
            {posix char class name} { {quantified {@NCC:<[:alnum:]>} 1} }
            quote { {priority @CHR:<'> 0} }
            {result item descriptor} {
                {priority @STR:<start> 0}
                {priority @STR:<length> 0}
                {priority @STR:<g1start> 0}
                {priority @STR:<g1length> 0}
                {priority @STR:<name> 0}
                {priority @STR:<lhs> 0}
                {priority @STR:<symbol> 0}
                {priority @STR:<rule> 0}
                {priority @STR:<value> 0}
                {priority @STR:<values> 0}
            }
            {result item descriptor list} { {quantified {result item descriptor} 0
                    separator {{result item descriptor separator} 0}} }
            {result item descriptor separator} {
                {priority @CHR:<,> 0}
                {priority {
                        @CHR:<,>
                        whitespace} 0}
            }
            {safe cc character} { {priority {{@^CLS:<\n-\r\135\205\u2028-\u2029>}} 0} }
            sign { {priority @CLS:<+-> 0} }
            {string without single quote or vertical space} { {quantified {@^CLS:<\n-\r'\u2028-\u2029>} 1} }
            {terminated hash comment} { {priority {
                    @CHR:<#>
                    {hash comment body}
                    {vertical space char}} 0} }
            {unterminated final hash comment} { {priority {
                    @CHR:<#>
                    {hash comment body}} 0} }
            {vertical space char} { {priority {{@CLS:<\n-\r\u2028-\u2029>}} 0} }
            {zero or more word characters} { {quantified @CHR:<w> 0} }
        }
        discard {
            {hash comment} {
                {priority {{terminated hash comment}} 0}
                {priority {{unterminated final hash comment}} 0}
            }
            whitespace { {quantified @CHR:<s> 1} }
        }
