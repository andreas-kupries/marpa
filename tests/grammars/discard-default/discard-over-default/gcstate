grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority fox 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            fox { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        discard {
            bar { {priority @LIT:<b> 0} }
            foo { {priority @LIT:<a> 0} }
        }
        events {
            bar {
                discard {
                    wolf on
                }
            }
            foo {
                discard {
                    fox on
                }
            }
        }
        latm {
            fox 1
        }
        lexeme {
            fox { {priority @LIT:<x> 0} }
        }
        literal {
            @LIT:<a> { {string 97 0} }
            @LIT:<b> { {string 98 0} }
            @LIT:<x> { {string 120 0} }
        }
    }
}