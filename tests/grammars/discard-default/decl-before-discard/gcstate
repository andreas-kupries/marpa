grammar {
    global {
        start A
        inaccessible warn
    }
    g1 {
        {} {
            A { {priority foo 0
                    action {array values}
                    assoc left
                    bless {special undef}
                    mask 0} }
        }
        terminal {
            foo { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        discard {
            fox { {priority @LIT:<x> 0} }
        }
        events {
            fox {
                discard {
                    gone on
                }
            }
        }
        latm {
            foo 1
        }
        lexeme {
            foo { {priority @LIT:<a> 0} }
        }
        literal {
            @LIT:<a> { {string 97 0} }
            @LIT:<x> { {string 120 0} }
        }
    }
}