grammar {
    global {
        start B
        inaccessible warn
    }
    g1 {
        {} {
            B { {priority A 0
                    action {array values}
                    assoc left
                    mask 0} }
        }
        terminal {
            A { terminal }
        }
    }
    lexeme {
        action {array values}
    }
    l0 {
        events {
            A {
                before {
                    snafu off
                }
            }
        }
        lexeme {
            A { {priority @CHR:<a> 0} }
        }
        literal {
            @CHR:<a> { {character 97} }
        }
    }
}