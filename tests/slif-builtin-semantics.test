## API to the semantics-core

proc label  {} { return "builtin" }
proc pclass {} { return marpa::slif::parser }

proc result {base} {
    foreach file {
	ctrace_tcl
	ctrace
    } {
	set path [file join $base $file]
	if {![file exists $path]} continue
	return $path
    }
    return -code error "Unable to find result for $base"
}

proc initialize {} {}
proc finalize   {} {}

