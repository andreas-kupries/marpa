            {one or more word characters} { {quantified {@CLS:<_[:alnum:]>} 1} }
            {Perl identifier} { {priority {{one or more word characters}} 0} }
            {plain cc char} { {priority {{@^CLS:<\7-\r-\134-\135\u2028-\u2029>}} 0} }
            {plain cc char without hex} { {priority {{@^CLS:<\7-\r-\134-\135\u2028-\u2029[:xdigit:]>}} 0} }
            {plain cc char without octal} { {priority {{@^CLS:<\7-\r-0-7\134-\135\u2028-\u2029>}} 0} }
            {plain cc element} {
                {priority {
                        {plain cc char}
                        {cc range completion}} 0}
                {priority {
                        {posix char class}
                        {nullable cc}} 0}
            }
            {plain cc element without hex} {
                {priority {
                        {plain cc char without hex}
                        {cc range completion}} 0}
                {priority {
                        {posix char class}
                        {nullable cc}} 0}
            }
            {plain cc element without octal} {
                {priority {
                        {plain cc char without octal}
                        {cc range completion}} 0}
                {priority {
                        {posix char class}
                        {nullable cc}} 0}
            }
            {plain string char} { {priority {{@^CLS:<\7-\10\n-\r'\134\u2028-\u2029>}} 0} }
            {plain string char without hex} { {priority {{@^CLS:<\7-\10\n-\r'\134\u2028-\u2029[:xdigit:]>}} 0} }
            {plain string char without octal} { {priority {{@^CLS:<\7-\10\n-\r'0-7\134\u2028-\u2029>}} 0} }
            {posix char class} { {priority {
                    {@STR:<\133:>}
                    {posix char class name}
                    {@STR:<:\135>}} 0} }
            {posix char class name} { {quantified {@NCC:<[:alnum:]>} 1} }
            {result item descriptor} {
                {priority @STR:<start> 0}
                {priority @STR:<length> 0}
                {priority @STR:<g1start> 0}
                {priority @STR:<g1length> 0}
                {priority @STR:<name> 0}
                {priority @STR:<lhs> 0}
                {priority @STR:<symbol> 0}
                {priority @STR:<rule> 0}
                {priority @STR:<value> 0}
                {priority @STR:<values> 0}
                {priority @STR:<ord> 0}
            }
            {result item descriptor list} { {quantified {result item descriptor} 0
                    separator {{result item descriptor separator} 0}} }
            {result item descriptor separator} {
                {priority @CHR:<,> 0}
                {priority {
                        @CHR:<,>
                        whitespace} 0}
            }
            sign { {priority @CLS:<+-> 0} }
            {string close} { {priority @CHR:<'> 0} }
            {string open} { {priority @CHR:<'> 0} }
            {terminated hash comment} { {priority {
                    @CHR:<#>
                    {hash comment body}
                    {vertical space char}} 0} }
            {unterminated final hash comment} { {priority {
                    @CHR:<#>
                    {hash comment body}} 0} }
            {vertical space char} { {priority {{@CLS:<\n-\r\u2028-\u2029>}} 0} }
            {zero or more word characters} { {quantified {@CLS:<_[:alnum:]>} 0} }
        }
        discard {
            {hash comment} {
                {priority {{terminated hash comment}} 0}
                {priority {{unterminated final hash comment}} 0}
            }
            whitespace { {quantified {@NCC:<[:space:]>} 1} }
        }
        latm {
            @LEX:@CHR:<*> 1
            @LEX:@CHR:<+> 1
            @LEX:@CHR:<,> 1
            @LEX:@CHR:<=> 1
            {@LEX:@CHR:<\50>} 1
            {@LEX:@CHR:<\51>} 1
            {@LEX:@CHR:<\73>} 1
            {@LEX:@CHR:<\173>} 1
            {@LEX:@CHR:<\175>} 1
            @LEX:@STR:<:default> 1
            @LEX:@STR:<:discard> 1
            @LEX:@STR:<:lexeme> 1
            @LEX:@STR:<:start> 1
            @LEX:@STR:<=>> 1
            @LEX:@STR:<action> 1
            @LEX:@STR:<assoc> 1
            @LEX:@STR:<by> 1
            @LEX:@STR:<completed> 1
            @LEX:@STR:<current> 1
            @LEX:@STR:<default> 1
            @LEX:@STR:<discard> 1
            @LEX:@STR:<event> 1
            @LEX:@STR:<fatal> 1
            @LEX:@STR:<forgiving> 1
            @LEX:@STR:<group> 1
            @LEX:@STR:<high> 1
            @LEX:@STR:<inaccessible> 1
            @LEX:@STR:<is> 1
            @LEX:@STR:<latm> 1
            @LEX:@STR:<left> 1
            @LEX:@STR:<lexeme> 1
            @LEX:@STR:<lexer> 1
            @LEX:@STR:<low> 1
            @LEX:@STR:<name> 1
            @LEX:@STR:<null-ranking> 1
            @LEX:@STR:<null> 1
            @LEX:@STR:<nulled> 1
            @LEX:@STR:<off> 1
            @LEX:@STR:<ok> 1
            @LEX:@STR:<on> 1
            @LEX:@STR:<pause> 1
            @LEX:@STR:<predicted> 1
            @LEX:@STR:<priority> 1
            @LEX:@STR:<proper> 1
            @LEX:@STR:<rank> 1
            @LEX:@STR:<right> 1
            @LEX:@STR:<separator> 1
            @LEX:@STR:<start> 1
            @LEX:@STR:<symbol> 1
            @LEX:@STR:<warn> 1
            {array descriptor} 1
            {bare name} 1
            {before or after} 1
            boolean 1
            {bracketed name} 1
            {character class} 1
            {op declare bnf} 1
            {op declare match} 1
            {op equal priority} 1
            {op loosen} 1
            {Perl name} 1
            {reserved action name} 1
            {reserved event name} 1
            {signed integer} 1
            {single quoted name} 1
            {single quoted string} 1
            {standard name} 1
        }
        lexeme {
            @LEX:@CHR:<*> { {priority @CHR:<*> 0} }
            @LEX:@CHR:<+> { {priority @CHR:<+> 0} }
            @LEX:@CHR:<,> { {priority @CHR:<,> 0} }
            @LEX:@CHR:<=> { {priority @CHR:<=> 0} }
            {@LEX:@CHR:<\50>} { {priority {{@CHR:<\50>}} 0} }
            {@LEX:@CHR:<\51>} { {priority {{@CHR:<\51>}} 0} }
            {@LEX:@CHR:<\73>} { {priority {{@CHR:<\73>}} 0} }
            {@LEX:@CHR:<\173>} { {priority {{@CHR:<\173>}} 0} }
            {@LEX:@CHR:<\175>} { {priority {{@CHR:<\175>}} 0} }
            @LEX:@STR:<:default> { {priority @STR:<:default> 0} }
            @LEX:@STR:<:discard> { {priority @STR:<:discard> 0} }
            @LEX:@STR:<:lexeme> { {priority @STR:<:lexeme> 0} }
            @LEX:@STR:<:start> { {priority @STR:<:start> 0} }
            @LEX:@STR:<=>> { {priority @STR:<=>> 0} }
            @LEX:@STR:<action> { {priority @STR:<action> 0} }
            @LEX:@STR:<assoc> { {priority @STR:<assoc> 0} }
            @LEX:@STR:<by> { {priority @STR:<by> 0} }
            @LEX:@STR:<completed> { {priority @STR:<completed> 0} }
            @LEX:@STR:<current> { {priority @STR:<current> 0} }
            @LEX:@STR:<default> { {priority @STR:<default> 0} }
            @LEX:@STR:<discard> { {priority @STR:<discard> 0} }
            @LEX:@STR:<event> { {priority @STR:<event> 0} }
            @LEX:@STR:<fatal> { {priority @STR:<fatal> 0} }
            @LEX:@STR:<forgiving> { {priority @STR:<forgiving> 0} }
            @LEX:@STR:<group> { {priority @STR:<group> 0} }
            @LEX:@STR:<high> { {priority @STR:<high> 0} }
            @LEX:@STR:<inaccessible> { {priority @STR:<inaccessible> 0} }
            @LEX:@STR:<is> { {priority @STR:<is> 0} }
            @LEX:@STR:<latm> { {priority @STR:<latm> 0} }
            @LEX:@STR:<left> { {priority @STR:<left> 0} }
            @LEX:@STR:<lexeme> { {priority @STR:<lexeme> 0} }
            @LEX:@STR:<lexer> { {priority @STR:<lexer> 0} }
            @LEX:@STR:<low> { {priority @STR:<low> 0} }
            @LEX:@STR:<name> { {priority @STR:<name> 0} }
            @LEX:@STR:<null-ranking> { {priority @STR:<null-ranking> 0} }
            @LEX:@STR:<null> { {priority @STR:<null> 0} }
            @LEX:@STR:<nulled> { {priority @STR:<nulled> 0} }
            @LEX:@STR:<off> { {priority @STR:<off> 0} }
            @LEX:@STR:<ok> { {priority @STR:<ok> 0} }
            @LEX:@STR:<on> { {priority @STR:<on> 0} }
            @LEX:@STR:<pause> { {priority @STR:<pause> 0} }
            @LEX:@STR:<predicted> { {priority @STR:<predicted> 0} }
            @LEX:@STR:<priority> { {priority @STR:<priority> 0} }
            @LEX:@STR:<proper> { {priority @STR:<proper> 0} }
            @LEX:@STR:<rank> { {priority @STR:<rank> 0} }
            @LEX:@STR:<right> { {priority @STR:<right> 0} }
            @LEX:@STR:<separator> { {priority @STR:<separator> 0} }
            @LEX:@STR:<start> { {priority @STR:<start> 0} }
            @LEX:@STR:<symbol> { {priority @STR:<symbol> 0} }
            @LEX:@STR:<warn> { {priority @STR:<warn> 0} }
            {array descriptor} { {priority {
                    {array descriptor left bracket}
                    {result item descriptor list}
                    {array descriptor right bracket}} 0} }
            {bare name} { {priority {{one or more word characters}} 0} }
            {before or after} {
                {priority @STR:<before> 0}
                {priority @STR:<after> 0}
            }
            boolean { {priority @RAN:<01> 0} }
            {bracketed name} { {priority {
                    @CHR:<<>
                    {bracketed name string}
                    @CHR:<>>} 0} }
            {character class} { {priority {
                    {a character class}
                    {character class modifiers}} 0} }
            {op declare bnf} { {priority @STR:<::=> 0} }
            {op declare match} { {priority @CHR:<~> 0} }
            {op equal priority} { {priority @CHR:<|> 0} }
            {op loosen} { {priority @STR:<||> 0} }
            {Perl name} { {quantified {Perl identifier} 1
                    separator {{double colon} 1}} }
            {reserved action name} { {priority {
                    {double colon}
                    {one or more word characters}} 0} }
            {reserved event name} { {priority {
                    {double colon}
                    {one or more word characters}} 0} }
            {signed integer} {
                {priority integer 0}
                {priority {
                        sign
                        integer} 0}
            }
            {single quoted name} { {priority {{a single quoted string}} 0} }
            {single quoted string} { {priority {
                    {a single quoted string}
                    {character class modifiers}} 0} }
            {standard name} { {priority {
                    @CLS:<A-Za-z>
                    {zero or more word characters}} 0} }
        }
        literal {
            {@^CLS:<\7-\10\n-\r'0-7\134\u2028-\u2029>} { {^charclass {7 8} {10 13} 39 {48 55} 92 {8232 8233}} }
            {@^CLS:<\7-\10\n-\r'\134\u2028-\u2029>} { {^charclass {7 8} {10 13} 39 92 {8232 8233}} }
            {@^CLS:<\7-\10\n-\r'\134\u2028-\u2029[:xdigit:]>} { {^charclass {7 8} {10 13} 39 92 {8232 8233} xdigit} }
            {@^CLS:<\7-\r-0-7\134-\135\u2028-\u2029>} { {^charclass {7 13} 45 {48 55} {92 93} {8232 8233}} }
            {@^CLS:<\7-\r-\134-\135\u2028-\u2029>} { {^charclass {7 13} 45 {92 93} {8232 8233}} }
            {@^CLS:<\7-\r-\134-\135\u2028-\u2029[:xdigit:]>} { {^charclass {7 13} 45 {92 93} {8232 8233} xdigit} }
            {@^CLS:<\n-\r\u2028-\u2029>} { {^charclass {10 13} {8232 8233}} }
            @CHR:<#> { {character 35} }
            @CHR:<'> { {character 39} }
            @CHR:<*> { {character 42} }
            @CHR:<+> { {character 43} }
            @CHR:<,> { {character 44} }
            @CHR:<-> { {character 45} }
            @CHR:<1> { {character 49} }
            @CHR:<:> { {character 58} }
            @CHR:<<> { {character 60} }
            @CHR:<=> { {character 61} }
            @CHR:<>> { {character 62} }
            {@CHR:<\50>} { {character 40} }
            {@CHR:<\51>} { {character 41} }
            {@CHR:<\73>} { {character 59} }
            {@CHR:<\133>} { {character 91} }
            {@CHR:<\134>} { {character 92} }
            {@CHR:<\135>} { {character 93} }
            {@CHR:<\173>} { {character 123} }
            {@CHR:<\175>} { {character 125} }
            @CHR:<^> { {character 94} }
            @CHR:<a> { {character 97} }
            @CHR:<b> { {character 98} }
            @CHR:<c> { {character 99} }
            @CHR:<d> { {character 100} }
            @CHR:<e> { {character 101} }
            @CHR:<f> { {character 102} }
            @CHR:<g> { {character 103} }
            @CHR:<h> { {character 104} }
            @CHR:<i> { {character 105} }
            @CHR:<k> { {character 107} }
            @CHR:<l> { {character 108} }
            @CHR:<m> { {character 109} }
            @CHR:<n> { {character 110} }
            @CHR:<o> { {character 111} }
            @CHR:<p> { {character 112} }
            @CHR:<r> { {character 114} }
            @CHR:<s> { {character 115} }
            @CHR:<t> { {character 116} }
            @CHR:<u> { {character 117} }
            @CHR:<v> { {character 118} }
            @CHR:<w> { {character 119} }
            @CHR:<x> { {character 120} }
            @CHR:<y> { {character 121} }
            @CHR:<|> { {character 124} }
            @CHR:<~> { {character 126} }
            @CLS:<+-> { {charclass 43 45} }
            {@CLS:<\134a-bfnrtv>} { {charclass 92 {97 98} 102 110 114 116 118} }
            {@CLS:<\n-\r\u2028-\u2029>} { {charclass {10 13} {8232 8233}} }
            {@CLS:<_[:alnum:]>} { {charclass 95 alnum} }
            {@CLS:<_[:alnum:][:space:]>} { {charclass 95 alnum space} }
            @CLS:<A-Za-z> { {charclass {65 90} {97 122}} }
            {@NCC:<[:alnum:]>} { {named-class alnum} }
            {@NCC:<[:digit:]>} { {named-class digit} }
            {@NCC:<[:space:]>} { {named-class space} }
            {@NCC:<[:xdigit:]>} { {named-class xdigit} }
            @RAN:<01> { {range 48 49} }
            @RAN:<03> { {range 48 51} }
            @RAN:<07> { {range 48 55} }
        }
    }
