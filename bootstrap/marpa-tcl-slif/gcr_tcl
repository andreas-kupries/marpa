                        mask 0
                        name statement/4}
                {priority {{quantified rule}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/5}
                {priority {{discard rule}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/6}
                {priority {{default rule}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/7}
                {priority {{lexeme default statement}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/8}
                {priority {{discard default statement}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/9}
                {priority {{lexeme rule}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/10}
                {priority {{completion event declaration}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/11}
                {priority {{nulled event declaration}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/12}
                {priority {{prediction event declaration}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/13}
                {priority {{current lexer statement}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/14}
                {priority {{inaccessible statement}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name statement/15}
            }
            {statement group} { {priority {
                    {@LEX:@CHR:<\173>}
                    statements
                    {@LEX:@CHR:<\175>}} 0
                        action {array {name values}}
                        assoc left
                        mask {1 0 0}
                        name {statement group/0}} }
            statements { {quantified statement 1
                    action {array {name values}}
                    name statements/0} }
            symbol { {priority {{symbol name}} 0
                    action {array {name values}}
                    assoc left
                    mask 0
                    name symbol/0} }
            {symbol name} {
                {priority {{bare name}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name {symbol name/0}}
                {priority {{bracketed name}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name {symbol name/1}}
