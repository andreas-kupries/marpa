grammar {
    global {
        start statements
        inaccessible warn
    }
    g1 {
        {} {
            action { {priority {
                    @LEX:@STR:<action>
                    @LEX:@STR:<=>>
                    {action name}} 0
                        action {array {name values}}
                        assoc left
                        mask {1 1 0}
                        name action/0} }
            {action name} {
                {priority {{Perl name}} 0
                        action {array {name values}}
                        assoc left
                        mask 0
                        name {action name/0}}
