	    @STR:<symbol>
	    @STR:<value>
	    @STR:<values>
	    @STR:<warn>
	    @STR:<||>
	    {a character class}
	    {a single quoted string}
	    {array descriptor left bracket}
	    {array descriptor right bracket}
	    {bracketed name string}
	    {cc close}
	    {cc element and more}
	    {cc open}
	    {cc range completion}
