	    {plain cc char without octal}
	    {plain cc element}
	    {plain cc element without hex}
	    {plain cc element without octal}
	    {plain string char}
	    {plain string char without hex}
	    {plain string char without octal}
	    {posix char class}
	    {posix char class name}
	    RAN<d56367-d56373>
	    RAN<d56478-d56516>
	    RAN<d56480-d56516>
	    RAN<d56499-d56570>
	    RAN<d56517-d56861>
